{
    "name": "hilmy/feature-control",
    "description": "Helper class for controlling features",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "feature",
        "flag",
        "percentage",
        "whitelist"
    ],
    "autoload": {
        "psr-4": {
            "Hilmy\\FeatureControl\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "mhilmyh",
            "email": "mhilmy021@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Hilmy\\ServiceProviders\\FeatureControl"
            ],
            "aliases": {
                "Feature": "Hilmy\\FeatureControl\\Facades\\Feature"
            }
        }
    },
    "require": {
        "laravel/framework": "^9.44"
    }
}
